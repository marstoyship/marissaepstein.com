.project-casestudy.edtrips

  .project-hero
    background: rgba(79, 185, 225, 1) url('/img/bg-edtrips.jpg') no-repeat
    background-size: cover
    background-attachment: fixed
    text-shadow: 0px 0px 10px rgba(73, 73, 73, 0.5)  
  
  .project-section.project-description
    a
      color: #e24f2a

  .project-section.print
    group()

  &.outline
    a
      transition-property: color
      transition-duration: 200ms

    path
      transition-property: stroke
      transition-duration: 200ms 